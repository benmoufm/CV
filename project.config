[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group review/training
	owner = group user/benmoufm
[access "refs/for/refs/*"]
	read = group review/training
	read = group user/benmoufm
	push = group review/training
	push = group user/benmoufm
	label-Code-Review = -2..+2 group review/training
	label-Code-Review = -2..+2 group user/benmoufm
	label-Verified = -1..+1 group review/training
	label-Verified = -1..+1 group user/benmoufm
	rebase = group review/training
	rebase = group user/benmoufm
	removeReviewer = group review/training
	removeReviewer = group user/benmoufm
	submit = group review/training
	exclusiveGroupPermissions = read push label-Code-Review label-Verified rebase removeReviewer submit
[access "refs/heads/*"]
	read = group review/training
	read = group user/benmoufm
	create = group review/training
	create = group user/benmoufm
	push = +force group review/training
	push = +force group user/benmoufm
	pushMerge = group review/training
	pushMerge = group user/benmoufm
	exclusiveGroupPermissions = read create push pushMerge
[access "refs/tags/*"]
	push = +force group review/training
	push = group user/benmoufm
	push = +force group user/benmoufm
	exclusiveGroupPermissions = push
